{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "rules": {
            "semi": ["error", "always"],
            "quotes": ["error", "double"]
        }
    }
}
